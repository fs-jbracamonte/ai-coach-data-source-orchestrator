{
  "name": "data-source-orchestrator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:name-matcher && npm run test:error-handling && npm run test:date-extraction && npm run test:content-search",
    "test:name-matcher": "node test/test-name-matcher.js",
    "test:error-handling": "node test/test-error-handling.js",
    "test:transcript-conversion": "node test/test-transcript-conversion.js",
    "test:download-conversion": "node test/test-download-conversion.js",
    "test:datasource-mapping": "node test/test-datasource-mapping.js",
    "test:all-datasource-generators": "node test/test-all-datasource-generators.js",
    "test:date-extraction": "node test/test-date-extraction.js",
    "test:content-search": "node test/test-content-search.js",
    "demo:name-matcher": "node test/demo-name-matcher.js",
    "jira:export": "node jira/export-to-csv.js",
    "jira:split": "node jira/split-by-assignee.js",
    "jira:markdown": "node jira/csv-to-markdown.js",
    "jira:team-report": "node jira/team-report.js",
    "jira:team-all": "node jira/run-team-report.js",
    "jira:all": "node jira/run-all.js",
    "transcripts:download": "node transcripts/download-from-drive.js",
    "transcripts:test-convert": "node test/test-transcript-conversion.js",
    "daily:query": "node daily-reports/db-query.js",
    "daily:convert": "node daily-reports/csv-to-markdown.js",
    "daily:all": "node daily-reports/run-all.js",
    "daily:clean": "node daily-reports/clean.js",
    "jira:clean": "node jira/clean.js",
    "transcripts:clean": "node transcripts/clean.js",
    "datasource:clean": "node datasource-generator/clean.js",
    "clean:all": "npm run daily:clean && npm run jira:clean && npm run transcripts:clean && npm run datasource:clean",
    "all": "npm run jira:all && npm run transcripts:download && npm run daily:all",
    "project1:daily": "cross-env CONFIG_FILE=config.project1.json npm run daily:all",
    "project1:jira": "cross-env CONFIG_FILE=config.project1.json npm run jira:all",
    "project1:jira-team": "cross-env CONFIG_FILE=config.project1.json npm run jira:team-all",
    "project1:transcripts": "cross-env CONFIG_FILE=config.project1.json npm run transcripts:download",
    "project1:all": "cross-env CONFIG_FILE=config.project1.json npm run all",
    "project2:daily": "cross-env CONFIG_FILE=config.project2.json npm run daily:all",
    "project2:jira": "cross-env CONFIG_FILE=config.project2.json npm run jira:all",
    "project2:jira-team": "cross-env CONFIG_FILE=config.project2.json npm run jira:team-all",
    "project2:transcripts": "cross-env CONFIG_FILE=config.project2.json npm run transcripts:download",
    "project2:all": "cross-env CONFIG_FILE=config.project2.json npm run all",
    "rocks:daily": "cross-env CONFIG_FILE=configs/rocks/config.json npm run daily:all",
    "rocks:jira": "cross-env CONFIG_FILE=configs/rocks/config.json npm run jira:all",
    "rocks:jira-team": "cross-env CONFIG_FILE=config.rocks.json npm run jira:team-all",
    "rocks:transcripts": "cross-env CONFIG_FILE=configs/rocks/config.json npm run transcripts:download",
    "rocks:datasource-team": "cross-env CONFIG_FILE=config.rocks.json npm run datasource:team",
    "rocks:datasource-weekly": "cross-env CONFIG_FILE=config.rocks.json npm run datasource:weekly-digest",
    "rocks:all": "cross-env CONFIG_FILE=config.rocks.json npm run all",
    "engagepath:daily": "cross-env CONFIG_FILE=configs/engagepath/config.json npm run daily:all",
    "engagepath:jira": "cross-env CONFIG_FILE=configs/engagepath/config.json npm run jira:all",
    "engagepath:jira-team": "cross-env CONFIG_FILE=config.engagepath.json npm run jira:team-all",
    "engagepath:transcripts": "cross-env CONFIG_FILE=configs/engagepath/config.json npm run transcripts:download",
    "engagepath:all": "cross-env CONFIG_FILE=config.engagepath.json npm run all",
    "use": "node run-with-config.js",
    
    "generate:1on1": "node datasource-generator/generate_datasources.js",
    "generate:team": "node datasource-generator/generate_team_datasource.js",
    "generate:weekly": "node datasource-generator/generate_weekly_digest.js",

    "rocks:1on1": "node datasource-generator/generate_datasources.js --team rocks --report 1on1",
    "rocks:team": "node datasource-generator/generate_team_datasource.js --team rocks --report team",
    "rocks:weekly": "node datasource-generator/generate_weekly_digest.js --team rocks --report weekly",
    "engagepath:1on1": "cross-env CONFIG_FILE=configs/engagepath/config.json node datasource-generator/generate_datasources.js",
    "engagepath:team": "node datasource-generator/generate_team_datasource.js --team engagepath --report team",
    "engagepath:weekly": "node datasource-generator/generate_weekly_digest.js --team engagepath --report weekly",

    "list-reports": "node -e \"const fs=require('fs');const p='configs';let teams=[];if(fs.existsSync(p)){teams=fs.readdirSync(p,{withFileTypes:true}).filter(d=>d.isDirectory()&&d.name!=='shared').map(d=>d.name);}console.log('Available teams:',teams.join(', ')||'(none)');console.log('Report types: 1on1, team, weekly');\"",
    "rocks:clean": "node -e \"const fs=require('fs');const p='datasource-generator/output/rocks';fs.rmSync(p,{recursive:true,force:true});console.log('Cleaned '+p);\"",
    "engagepath:clean": "node -e \"const fs=require('fs');const p='datasource-generator/output/engagepath';fs.rmSync(p,{recursive:true,force:true});console.log('Cleaned '+p);\"",

    "datasource:generate": "node datasource-generator/generate_datasources.js",
    "datasource:from-existing": "node datasource-generator/generate_from_existing.js",
    "datasource:team": "node datasource-generator/generate_team_datasource.js",
    "datasource:weekly-digest": "node datasource-generator/generate_weekly_digest.js",
    "datasource:all": "npm run datasource:generate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.2",
    "googleapis": "^160.0.0",
    "joi": "^18.0.1",
    "mysql2": "^3.11.0",
    "ssh2": "^1.15.0"
  },
  "devDependencies": {
    "cross-env": "^10.0.0"
  }
