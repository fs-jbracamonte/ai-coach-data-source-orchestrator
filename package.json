    "list-reports": "node -e \"const fs=require('fs');const p='configs';let teams=[];if(fs.existsSync(p)){teams=fs.readdirSync(p,{withFileTypes:true}).filter(d=>d.isDirectory()&&d.name!=='shared').map(d=>d.name);}console.log('Available teams:',teams.join(', ')||'(none)');console.log('Report types: 1on1, team, weekly, dashboard');\"",
